<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:sys="clr-namespace:System;assembly=System.Runtime"
                    xmlns:ut="clr-namespace:Fluent.UITests.TestUtilities">

    <ResourceDictionary.MergedDictionaries>
        
        <ut:TestDictionary Name="Light">
            <ut:TestDictionary.MergedDictionaries>
                <ut:TestDictionary Source="/Fluent.UITests;component/ResourceTests/Data/Light.Test.xaml" />

                <ut:TestDictionary Name="Default">
                    <!--Contextmenu-->
                    <Thickness x:Key="ListBoxBorderThemeThickness">0</Thickness>
                    <SolidColorBrush x:Key="ListBoxBackground" Color="#FFF2F2F2" />
                    <HorizontalAlignment x:Key="ListBox_HorizontalAlignment">Stretch</HorizontalAlignment>
                    <VerticalAlignment x:Key="VerticalContentAlignment">Center</VerticalAlignment>
                    <VerticalAlignment x:Key="ListBox_VerticalAlignment">Stretch</VerticalAlignment>
                    <Thickness x:Key="ListBoxPadding">10</Thickness>
                    <HorizontalAlignment x:Key="HorizontalContentAlignment">Left</HorizontalAlignment>
                    <SolidColorBrush x:Key="TextFillColorPrimaryBrush" Color="{StaticResource TextFillColorPrimary}" />
                    <sys:Double x:Key="ListBox_Minwidth">0</sys:Double>
                    <sys:Double x:Key="ListBox_MinHeight">0</sys:Double>
                    <sys:Boolean x:Key="ListBoxCanContentScroll">True</sys:Boolean>
                    
                    <!--ListBoxItem properties--><!--
                    <SolidColorBrush x:Key="ContentBorder_ListBoxItemForeground" Color="{StaticResource TextFillColorPrimaryBrush}" />
                    <SolidColorBrush x:Key="ContentBorder_ListBoxItemBackground" Color="Transparent" />
                    <Thickness x:Key="ContentBorder_ListBoxItemPadding">12</Thickness>
                    <Thickness x:Key="ContentBorder_ListBoxItemMargin">10</Thickness>
                    <Thickness x:Key="ContentBorder_ListBoxItemBorderThickness">10</Thickness>
                    <SolidColorBrush x:Key="ContentBorder_ListBoxItemBorderBrush" Color="Transparent" />
                    <CornerRadius x:Key="ContentBorder_ListBoxItemCornerRadius">15</CornerRadius>
                    <HorizontalAlignment x:Key="ContentBorder_ListBoxIem_HorizontalAlignment">Stretch</HorizontalAlignment>
                    <VerticalAlignment x:Key="ContentBorder_ListBoxIem_VerticalContentAlignment">Center</VerticalAlignment>
                    <VerticalAlignment x:Key="ContentBorder_ListBoxIem_VerticalAlignment">bottom</VerticalAlignment>
                    <HorizontalAlignment x:Key="ContentBorder_ListBoxIem_HorizontalContentAlignment">Left</HorizontalAlignment>-->


                    <!--PART_ContentHost : ScrollViewer-->
                    <VerticalAlignment x:Key="PART_ContentHost_ListBox_VerticalAlignment">Stretch</VerticalAlignment>
                    <sys:Boolean x:Key="PART_ContentHost_ListBox_CanContentScroll">True</sys:Boolean>
                    <ScrollBarVisibility x:Key="PART_ContentHost_ListBox_HorizontalScrollBarVisibility">Auto</ScrollBarVisibility>
                    <ScrollBarVisibility x:Key="PART_ContentHost_ListBox_VerticalScrollBarVisibility">Auto</ScrollBarVisibility>
                    <Thickness x:Key="PART_ContentHost_ListBox_Padding">0</Thickness>


                      
                </ut:TestDictionary>
                
                <ut:TestDictionary Name="CustomListbox">
                    <!--<SolidColorBrush x:Key="TextControlBackground" Color="Aqua" />
      -->
                    <!--Newly added properties -->
                    <Thickness x:Key="ListBoxPadding">4</Thickness>

                    <sys:Double x:Key="ListBox_Minwidth">100</sys:Double>
                    <sys:Double x:Key="ListBox_MinHeight">30</sys:Double>
                    <SolidColorBrush x:Key="TextFillColorPrimaryBrush" Color="Red" />
                    <SolidColorBrush x:Key="ListBoxBackground" Color="Black" />
                    <!--<SolidColorBrush x:Key="ContentBorder_TextControlBackground" Color="Black" />-->
                    <HorizontalAlignment x:Key="ListBox_HorizontalAlignment">Left</HorizontalAlignment>
                    <HorizontalAlignment x:Key="HorizontalContentAlignment">Left</HorizontalAlignment>
                    <VerticalAlignment x:Key="ListBox_VerticalAlignment">Top</VerticalAlignment>
                    <VerticalAlignment x:Key="VerticalContentAlignment">bottom</VerticalAlignment>
                    <!--<Visibility x:Key="ClearButton_Visibility">Visible</Visibility>-->
                </ut:TestDictionary>


            </ut:TestDictionary.MergedDictionaries>
        </ut:TestDictionary>

